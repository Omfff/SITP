<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a.fd">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <receiver
            android:name=".searchView.FoodSearchView$MyBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.a.fd.MY_BROADCAST" />
            </intent-filter>
        </receiver>

        <activity android:name=".CompareSearch" />
        <activity android:name=".compareSearch.CompareView" />
        <activity android:name=".searchView.FoodSearchView" />
        <activity android:name=".Test" />
        <activity android:name=".recordDiet.RecordDiet" />
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".searchView.FoodDetailsView" />
        <activity android:name=".foodCategory.FoodCategory" />
        <activity android:name=".FoodScan"></activity>
    </application>

</manifest>